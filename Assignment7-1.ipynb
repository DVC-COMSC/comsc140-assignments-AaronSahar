    "list = []\n",
    "average = 0\n",
    "for i in range(10):\n",
    "    list.append(int(input(\"Enter a number\")))\n",
    "    average += list[i]\n",
    "average /= 10\n",
    "for i in range(10):\n",
    "    list[i] -= average\n",
    "    if list[i] < 0:\n",
    "        list[i] *= -1\n",
    "for i in range(10):\n",
    "    print(f'{list[i]:.2f}', end=' ')\n",
    "print(average)\n"
   ]
  }
