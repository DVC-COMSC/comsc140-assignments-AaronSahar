    "import random\n",
    "numbers = []\n",
    "sum = 0\n",
    "i = 0\n",
    "while sum < 100:\n",
    "    numbers.append(random.randint(0,100))\n",
    "    sum += numbers[i]\n",
    "    i+=1\n",
    "i=0\n",
    "print(\"The odd numbers are:\", end = ' ')\n",
    "while i < len(numbers):\n",
    "    print(numbers[i], end = ' ')\n",
    "    i+=1\n",
    "print()\n",
    "print(\"The total sum is\", sum)"
